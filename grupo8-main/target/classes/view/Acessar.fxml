<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane
        xmlns="http://javafx.com/javafx/21.0.2"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="Classes.Dados"
        prefHeight="400.0"
        prefWidth="600.0"
        style="-fx-background-color: purple;">
   <children>
      <TextField fx:id="CampoCpf"  alignment="CENTER" layoutX="227.0" layoutY="151.0" prefHeight="25.0" prefWidth="149.0" promptText="Digite o seu CPF" />
      <TextField fx:id="CampoSenha"  alignment="CENTER" layoutX="226.0" layoutY="188.0" promptText="Digite a  sua senha" />
      <Button fx:id="acessar" onAction="#conferirAcesso" layoutX="274.0" layoutY="290.0" mnemonicParsing="false" text="Acessar" />
   </children>
</Pane>
